<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include
            domain="sharedpref"
            path="." />
        <exclude
            domain="database"
            path="safeguard_database" />
        <exclude
            domain="sharedpref"
            path="user_credentials.xml" />
    </cloud-backup>

    <device-transfer>
        <include
            domain="sharedpref"
            path="." />
        <exclude
            domain="database"
            path="safeguard_database" />
    </device-transfer>
</data-extraction-rules>